import "std/Prelude.oj" 
import "std/List.oj" 
import "std/Control.oj"
import "std/Write.oj"

def fizzBuzz(i)
    forEach(count(i)) { (x)
        if (x % 3 == 0 && x % 5 == 0) {
            write("FizzBuzz")
        }.elif (x % 3 == 0) {
            write("Fizz")
        }.elif (x % 5 == 0) {
            write("Buzz")
        }.else {
            write(x)
        }
    }

def logFizzBuzz(i)
    writeLog(fizzBuzz(i))

def listFizzBuzz(i)
    writeList(nil, fizzBuzz(i))
